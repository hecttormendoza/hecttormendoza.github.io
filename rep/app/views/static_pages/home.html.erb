                  <%= image_tag "telefonocontacto.svg", class: "img-contact" %>
                </div>
                <div class="col-8 col-md-10 align-self-center">
                  <a href="tel:5536662076" class="margin-0 gotham-book black">55 3666 2076</a>
                </div>
              </div>
            </div>
          </div>
          <div class="col-12 col-md-6 mx-auto">
            <%= form_tag contact_path, method: :post, class: 'contacto-form' do %>
            <!-- <form action="" class="contacto-form"> -->
              <%= select_tag 'propiedad', options_for_select(@propiedades.map{|propiedad| [propiedad]}), prompt: "Propiedades", class: 'w-100 gotham-book text-capitalize', id: "selected" %>
              <!-- <input type="text" class="w-100" placeholder="Nombre"> -->
              <%= text_field_tag 'Name', nil, placeholder: "Nombre", class: 'w-100 gotham-book', required: true %>
              <!-- <input type="text" class="w-100" placeholder="Correo"> -->
              <%= text_field_tag 'email', nil, placeholder: "Correo", class: 'w-100 gotham-book', required: true %>
              <!-- <textarea name="" id="" cols="2" rows="10" class="w-100 h-25" placeholder="Comentarios"></textarea> -->
              <%= text_area_tag 'comments', nil, cols: 2, rows: 10, class: 'w-100 gotham-book h-20', placeholder: "Comentarios", required: true %>
              <!-- <input type="submit" text="enviar"> -->
              <%= submit_tag 'enviar', class: "submit gotham-book" %>
              <!-- </form> -->
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <aside class="d-none d-lg-block">
      <%= image_tag "logo-azul.svg", class: "img-fluid", id: "contacto-1" %>
    </aside>
  </section>